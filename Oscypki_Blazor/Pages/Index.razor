@page "/"
@inject IOscypekRepository _MockOscypekRepository

<Carousel/>

@foreach (var oscypek in oscypki)
{
    <OscypekCard Oscypki="oscypek"></OscypekCard>
}


@code {

    IEnumerable<Oscypki> oscypki;

    protected override async Task OnInitializedAsync()
    {
        oscypki = _MockOscypekRepository.AllOscypki;
    }
}